{
  "name": "blackwell",
  "version": "1.0.0-alpha",
  "description": "API for a medical clinic using clean architecture",
  "main": "index.js",
  "author": "help me again",
  "license": "Apache-2.0",
  "homepage": "https://github.com/helpmeagain/blackwell",
  "private": true,
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "prisma db push && node dist/src/presentation/main",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run --config ./vitest.config.e2e.mts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "tsc:check": "tsc --noEmit",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "npx ts-node -r tsconfig-paths/register scripts/seed.ts",
    "prisma:nuke": "npx ts-node -r tsconfig-paths/register scripts/nuke.ts"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.4",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "^6.14.0",
    "bcryptjs": "^3.0.2",
    "express": "^5.1.0",
    "ioredis": "^5.7.0",
    "nodemailer": "^6.10.1",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.16.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "zod": "^3.25.76",
    "zod-validation-error": "^3.5.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@faker-js/faker": "^9.9.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@swc/core": "^1.13.3",
    "@types/express": "^5.0.3",
    "@types/node": "^22.17.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/pdfkit": "^0.13.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^16.6.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "globals": "^16.3.0",
    "prisma": "^6.14.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "unplugin-swc": "^1.5.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}